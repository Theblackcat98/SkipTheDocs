# gimprc
#
# This is your personal GIMP initialization file.  It is run after
# the system-wide 'gimprc' file.  Any values set here will override
# the system-wide values.

(gimp-version "2.10")
(plug-in-path "/plug-ins:/plug-ins")
(module-path "/modules:/modules")
(brush-path "/brushes:/brushes")
(pattern-path "/patterns:/patterns")
(palette-path "/palettes:/palettes")
(gradient-path "/gradients:/gradients")
(font-path "/fonts:/fonts")

(tile-cache-size 1024)
(undo-levels 5)
(undo-memory-size 64)
(show-tool-tips yes)
(show-status-bar yes)
(show-rulers yes)
(show-scrollbars yes)
(show-selection yes)
(show-grid no)
(grid-spacing 10)
(grid-style solid)
(default-image-type RGB)
(default-image-width 256)
(default-image-height 256)
(default-resolution 72.0)
(default-unit pixels)
(default-dot-for-dot no)
(default-comment "Created with GIMP")
