# vlcrc
#
# This file is the main configuration file for VLC.
#
# You can edit it to change VLC settings.
# For example, you can change the audio output module:
# aout=alsa

[core]
# fullscreen
fullscreen=1

# start in minimal view
qt-minimal-view=1

# video output module
vout=vdpau

# audio output module
aout=pulse

# deinterlacing mode
deinterlace=0

# no-plugins-cache
no-plugins-cache=1
