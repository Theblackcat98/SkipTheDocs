[user]
    name = Your Name
    email = your.email@example.com
[core]
    editor = vim
    excludesfile = ~/.gitignore_global
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p
[color]
    ui = true
[push]
    default = simple
